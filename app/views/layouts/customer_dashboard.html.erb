<head>
  <title>Store</title>
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js" type="text/javascript"></script>
  <![endif]-->
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <style>
   .row-fluid .span2{background:#BBD8E9;height:90%;}
   .row-fluid .span10{background:#FAFAFA;height:auto;width:65%; }
   
  </style>
</head>
<body>
  <div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
      <div class="container">
        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </a>
        <a class="brand" href="<%=root_url%>"><b>Aloo.com</b></a>
        <div class="nav-collapse">
          <ul class="nav">
            <% if user_signed_in? %>
            <li><a href=""> Logged in as <%= current_user.email %></a></li>
               <%if current_user.customer?%>
            <li><%=link_to 'My Dashboard',customer_listing_path%></li>
               <%elsif current_user.vendor?%>
            <li><%=link_to 'My Dashboard',vendor_index_path%></li>
               <%end%>
            <li><%= link_to 'Edit profile', edit_user_registration_path %></li>
            <li><%= link_to "Logout", destroy_user_session_path, :method=>:delete %></li>
            <% else %>
            <li><%= link_to "Sign up", new_user_registration_path %></li>
               nmbnmbnmb
                <li><%= link_to "Login", new_user_session_path %></li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
  
<div class="container-fluid">
    <div class="row-fluid">
    	
    <div class="span2">
   <div class="block1">
   	<div class="title">Menu
                 <div id="hello">
                   <%= render 'customer/menu_list' %>
                 </div>


       </div>
   	
   	
   </div>
   <div class="block2">
   		<div class="title">Near By Stores[category list]
             <div id="store_category">

             </div>
          </div>
   	
   </div>
    </div>
    <div class="span10">
    <ul class="breadcrumb">
   
    </ul>
    <%= yield %>
    </div>
   
    <div class="span2">
    	<div class="right_bar">
    		<div class="title">
    			Search
              <div id="items"> </div>
    		</div>
    	</div>
   
    </div>
    </div>
    </div>
</body>